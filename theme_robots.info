name = 'Theme Robots'
description = 'Custom Subtheme Implementation'
core = '7.x'
engine = 'phptemplate'
screenshot = 'screenshot.png'
base theme = 'omega'
version = '1.x'

; REGIONS
regions[header] = 'Header'
regions[branding] = 'Branding'
regions[menu] = 'Menu'
regions[content] = 'Content'
regions[footer] = 'Footer'

; ZONES
